# OpenAI API Key for the agent's language model
OPENAI_API_KEY=your_openai_api_key

# CDP (Coinbase Developer Platform) credentials
CDP_API_KEY_NAME=your_cdp_key_name
CDP_API_KEY_PRIVATE_KEY=your_cdp_private_key

# Contract Addresses
AI_AGENT_REGISTRY_ADDRESS=0x... # Address of the AIAgentRegistry contract
MUSIC_TOKEN_ADDRESS=0x...       # Address of the MusicToken contract

# Network Configuration
RPC_URL=your_rpc_url           # RPC endpoint for the blockchain network
NETWORK_ID=base-sepolia        # Network identifier (base-sepolia or local)

# Agent Configuration
AGENT_METADATA_URL=ipfs://...  # IPFS URL containing agent metadata
AGENT_INITIAL_STRATEGY="Recommend diverse music across genres with focus on user preferences"
AGENT_MIN_STAKE=100           # Minimum stake in MUSIC tokens
AGENT_INITIAL_FUNDING=150     # Initial funding amount in MUSIC tokens

# Funding Wallet Configuration
FUNDING_WALLET_PRIVATE_KEY=your_private_key  # Private key of the wallet that holds MUSIC tokens
FUNDING_WALLET_ADDRESS=0x...                 # Address of the funding wallet
